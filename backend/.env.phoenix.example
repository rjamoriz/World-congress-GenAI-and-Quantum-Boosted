# Phoenix Observability Configuration
# Copy this to your .env file and adjust values as needed

# Phoenix Server Configuration
PHOENIX_ENABLED=true
PHOENIX_ENDPOINT=http://localhost:6006
PHOENIX_PROJECT_NAME=world-congress-agenda-manager

# OpenTelemetry Configuration
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:6006/v1/traces
OTEL_SERVICE_NAME=agenda-manager-backend
OTEL_SERVICE_VERSION=1.0.0

# Phoenix Feature Flags
PHOENIX_TRACING=true
PHOENIX_METRICS=true
PHOENIX_DASHBOARD=true

# Phoenix Performance Settings
PHOENIX_SAMPLE_RATE=1.0
PHOENIX_METRICS_INTERVAL=30000
PHOENIX_PORT=6006

# Environment
NODE_ENV=development
